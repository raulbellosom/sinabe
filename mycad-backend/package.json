    "csv-parser": "^3.0.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.14",
    "pm2": "^5.4.1",
    "sharp": "^0.33.4",
    "uuid": "^10.0.0"
  },
